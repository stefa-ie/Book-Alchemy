SET publication_year = CASE
    WHEN title = 'Pride and Prejudice' THEN '1813-01-28'
    WHEN title = 'Sense and Sensibility' THEN '1811-10-30'
    WHEN title = 'Adventures of Huckleberry Finn' THEN '1884-12-10'
    WHEN title = 'The Adventures of Tom Sawyer' THEN '1876-06-17'
    WHEN title = '1984' THEN '1949-06-08'
    WHEN title = 'Animal Farm' THEN '1945-08-17'
    WHEN title = 'Mrs. Dalloway' THEN '1925-05-14'
    WHEN title = 'To the Lighthouse' THEN '1927-05-05'
    WHEN title = 'The Old Man and the Sea' THEN '1952-09-01'
    WHEN title = 'A Farewell to Arms' THEN '1929-09-27'
    ELSE publication_year -- Retain the current value if no match
END;
